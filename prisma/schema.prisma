
generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model chats {
  id String @id @default(uuid()) @db.Uuid
  device_id String
  message_text String
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt
}